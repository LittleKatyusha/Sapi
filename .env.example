# API Configuration
REACT_APP_API_URL=http://localhost:8080

# Security Configuration (Optional)
# Set to 'true' to disable right-click context menu in production
REACT_APP_DISABLE_RIGHT_CLICK=false

# Set to 'true' to disable developer shortcuts (F12, Ctrl+Shift+I, etc.) in production
REACT_APP_DISABLE_DEV_SHORTCUTS=false

# Set to 'true' to disable console logs in production
REACT_APP_DISABLE_CONSOLE=false

# Performance Configuration (Optional)
# Set to 'true' to enable performance monitoring
REACT_APP_MEASURE_PERFORMANCE=false

# Feature Flags (Optional)
# Set to 'true' to explicitly enable right-click (overrides security settings)
REACT_APP_ENABLE_RIGHT_CLICK=true

# Database Configuration (if needed)
# REACT_APP_DB_HOST=localhost
# REACT_APP_DB_PORT=5432

# Authentication Configuration (if needed)
# REACT_APP_AUTH_DOMAIN=your-auth-domain.com
# REACT_APP_CLIENT_ID=your-client-id

# Third-party Services (if needed)
# REACT_APP_ANALYTICS_ID=your-analytics-id
# REACT_APP_SENTRY_DSN=your-sentry-dsn
